{
    "name": "sibit-frontend",
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/cms/(.*)",
            "dest": "/cms/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}