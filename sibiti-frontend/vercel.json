{
  "version": 2,
  "builds": [{ "src": "build/index.html", "use": "@vercel/static-build" }],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*).(js|css|png|jpg|gif|ico|svg|json)",
      "dest": "/build/$1.$2",
      "headers": { "cache-control": "s-maxage=31536000,immutable" }
    },
    { "src": "/(.*).(woff|woff2|ttf|otf|eot)", "dest": "/build/$1.$2" },
    { "src": "/(favicon.ico)", "dest": "/build/$1" },
    { "src": "/manifest.json", "dest": "/build/manifest.json" },
    { "src": "/robots.txt", "dest": "/build/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/build/sitemap.xml" },
    { "src": "/sw.js", "dest": "/build/sw.js" },
    { "src": "/workbox-.*", "dest": "/build/$&" },
    { "src": "/(.*)", "dest": "/build/index.html" }
  ]
}
